{
  "manifest_version": 3,
  "name": "Google Meet Subtitle Extractor",
  "version": "1.0",
  "description": "Extracts and displays Google Meet subtitles in the console",
  "permissions": ["scripting"],
  "host_permissions": ["https://meet.google.com/*"],
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
